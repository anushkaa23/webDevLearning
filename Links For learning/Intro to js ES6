function fibonacciGenerator (n) {
//Do NOT change any of the code above ðŸ‘†
    if(n==1) return [0];
    if(n==2) return [0,1];
    var myList=[];
    myList[0]=0;
    myList[1]=1;
    for(var i=2;i<n;i++){
        myList[i]=myList[i-1]+myList[i-2];
    }
    return myList;
}

 